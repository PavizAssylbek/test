ER,MAAM,CAACS,UAAvB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,SAAS,EAAET,MAAM,CAACU,YAAvB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,GAAG,EAAC,aAAT;AAAuB,IAAA,GAAG,EAAC,GAA3B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mGAJF,CADF,EASE;AAAK,IAAA,SAAS,EAAEV,MAAM,CAACW,oBAAvB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,GAAG,EAAC,cAAT;AAAwB,IAAA,GAAG,EAAC,GAA5B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IADF,EAEE;AAAK,IAAA,GAAG,EAAC,mBAAT;AAA6B,IAAA,GAAG,EAAC,GAAjC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAFF,EAGE;AAAK,IAAA,GAAG,EAAC,oBAAT;AAA8B,IAAA,GAAG,EAAC,GAAlC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAHF,EAIE;AAAK,IAAA,GAAG,EAAC,qBAAT;AAA+B,IAAA,GAAG,EAAC,GAAnC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAJF,CATF,CADF,EAiBE;AAAG,IAAA,SAAS,EAAEX,MAAM,CAACY,kBAArB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2hEAjBF,EAoBE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kUApBF,EAqBE;AAAG,IAAA,SAAS,EAAEZ,MAAM,CAACa,cAArB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,opGArBF,EAwBE;AAAG,IAAA,SAAS,EAAEb,MAAM,CAACa,cAArB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yvDAxBF,EA2BE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4LA3BF,EA8BE;AAAG,IAAA,SAAS,EAAEb,MAAM,CAACa,cAArB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,grFA9BF,EAiCE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6fAjCF,EAoCE;AAAG,IAAA,SAAS,EAAEb,MAAM,CAACa,cAArB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0zCApCF,EAuCE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kRAvCF,EA0CE;AAAG,IAAA,SAAS,EAAEb,MAAM,CAACa,cAArB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,w1EA1CF,CADF,EA+CE;AAAK,IAAA,SAAS,EAAEb,MAAM,CAACc,YAAvB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAEIC,KAAK,CAAC,CAAD,CAAL,CACCC,IADD,CACM,CADN,EAECC,GAFD,CAEK,UAACC,CAAD,EAAGC,CAAH;AAAA,WACH,MAAC,WAAD;AAAa,MAAA,EAAE,EAAEA,CAAjB;AAAoB,MAAA,GAAG,EAAEA,CAAzB;AAA4B,MAAA,QAAQ,EAAE,IAAtC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADG;AAAA,GAFL,CAFJ,CA/CF,CAVF,CADF,CANF,CADF;AAgFD","sourcesContent":["import React from 'react'\r\nimport Head from 'next/head'\r\nimport SmallExtrim from '../../../components/SmallExtrim'\r\nimport MainLayout from '../../../components/MainLayout'\r\nimport styles from './styles.module.scss'\r\nimport clsx from 'clsx'\r\nimport Link from 'next/link'\r\n\r\nexport default function insideExpertOpinion() {\r\n\r\n\r\n\r\n  return (\r\n    <MainLayout>\r\n\r\n      <Head>\r\n        <title>Tourism: Project</title>\r\n      </Head>\r\n\r\n      <div className=\"flex\">\r\n        <div className={clsx(styles.inside, 'container')}>\r\n          <div className={styles.inside_head}>\r\n            <h2>Мнение эксперта о развитии туризма в Казахстане</h2>\r\n            <Link href=\"/project/extrim\">\r\n              <a className={styles.inside_link}>\r\n                <span>Назад</span> \r\n                <img src=\"/back.png\" alt=\"back\"/>\r\n              </a>\r\n            </Link>\r\n          </div>\r\n          <div className={styles.inside_content}>\r\n            <div className={styles.inside_left}>\r\n              <div className=\"flex\">\r\n                <div className={styles.inside_img}>\r\n                  <div className={styles.inside_small}>\r\n                    <img src=\"/extrim.jpg\" alt=\"#\"/>\r\n                  </div>\r\n                  <em>\r\n                    Марат Ахметжанов\r\n                  </em>\r\n                </div>\r\n                <div className={styles.inside_socialnetwork}>\r\n                  <img src=\"/logo-vk.svg\" alt=\"#\"/>\r\n                  <img src=\"/logo-twitter.svg\" alt=\"#\"/>\r\n                  <img src=\"/logo-facebook.svg\" alt=\"#\"/>\r\n                  <img src=\"/logo-instagram.svg\" alt=\"#\"/>\r\n                </div>\r\n              </div>\r\n              <p className={styles.inside_description}>\r\n                В Шымкенте прошла республиканская туристская ярмарка Silk Road Fair. В рамках ярмарки состоялся форум, на котором присутствовали представители ведущих европейских и казахстанских туристических компаний. Здесь рассматривались в том числе и вопросы важности маркетинговой деятельности для развития туризма в Казахстане. Об этом мы побеседовали с редактором британской компании Silk Road Media Маратом Ахметжановым.\r\n              </p>\r\n              <h6>О чем говорит статистика относительно туризма в Казахстане?</h6>\r\n              <p className={styles.inside_anytext}>\r\n                Предполагаю, что не всем она понравится. Между тем официальная статистика говорит о более чем миллионе туристов. В реальности это люди, пересекающие границу либо останавливающиеся в отелях, либо бизнес-туристы. Если мы говорим о туристах в чистом виде, которые купили тур и приехали в Казахстан, то я подозреваю, что их меньше, чем 80 тысяч человек в год. Это очень маленькое количество, учитывая тот потенциал, который может быть у Казахстана. Даже если предположить, как говорили на форуме, что приезжают два миллиона туристов, это все равно в десятки раз меньше, чем, например, у Великобритании, Таиланда, Турции, Франции и так далее.\r\n              </p>\r\n              <p className={styles.inside_anytext}>\r\n                За последние 12 лет мне, как представителю прессы и и человеку, который продвигает Центральную Азию, удалось побывать более чем в сорока государствах и лично наблюдать, как развивается туризм в этих странах и что привело к тому, что есть страны, которые получают до 40 миллионов туристов в год, а есть страны, которые не могут получить и 100 тысяч туристов.\r\n              </p>\r\n              <h6>\r\n                Почему? Чего не хватает Казахстану?\r\n              </h6>\r\n              <p className={styles.inside_anytext}>\r\n                В первую очередь нужно понимать, что Казахстан — это не тропическая страна с пляжами, теплым климатом двенадцать месяцев в году. Соответственно, курортный туризм на 90% можно исключить, кроме сезонного. И то он очень специализированный, для очень узкого круга людей, которые приедут в Боровое или на Каспийское море, или на Алаколь. Все равно большая часть туристов, которые хотят получить пляжно-курортный отдых, поедут на теплые моря. Поэтому рассматривать это как такую дистанцию для курортного туризма Казахстану сильно не стоит.\r\n              </p>\r\n              <h6>\r\n                На ваш взгляд, сколько времени понадобиться Казахстану, чтобы поднять туризм на должный уровень?\r\n              </h6>\r\n              <p className={styles.inside_anytext}>\r\n                Пять лет. Если сегодня начать уже делать все то, о чем я говорил выше: это проведение культурных мероприятий, вызывать инициативу на местах, проводить идеологическую работу по возрождению национальных обычаев, традиций культуры через активные мероприятия.\r\n              </p>\r\n              <h6>\r\n                Что вы можете сказать конкретно о Южном Казахстане?\r\n              </h6>\r\n              <p className={styles.inside_anytext}>\r\n                Я не скажу, что достаточно много, но знаю о достопримечательностях Южного Казахстана. Но историей мало кто может ознакомить туриста на месте. В том же городище «Отрар» нет даже вывесок, где было бы все написано, негде купить туристам сувениры и так далее. И получается, что историю сохранили, но никому это не нужно. И пока местное население в действительности не увидит в этом пользу и толк для самих себя и будет это любить, ни за какие деньги иностранцы сюда не приедут.\r\n              </p>\r\n            </div>\r\n            <div className={styles.inside_right}>\r\n              {\r\n                Array(5)\r\n                .fill(0)\r\n                .map((_,i) => (\r\n                  <SmallExtrim id={i} key={i} smallIMG={true} />\r\n                ))\r\n              }\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n    </MainLayout>\r\n  )\r\n}"]},"metadata":{},"sourceType":"module"}